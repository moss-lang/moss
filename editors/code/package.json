{
  "name": "quench",
  "displayName": "Quench",
  "description": "",
  "version": "0.1.1",
  "publisher": "samestep",
  "repository": "github:samestep/quench",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:quench"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "quench",
        "aliases": [
          "Quench",
          "quench"
        ],
        "extensions": [
          ".qn"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Quench configuration",
      "properties": {
        "quench.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.25",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.3.0",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.1"
  }
}
